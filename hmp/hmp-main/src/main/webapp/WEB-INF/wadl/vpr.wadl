<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://wadl.dev.java.net/2009/02"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xsi:schemaLocation="http://wadl.dev.java.net/2009/02 http://www.w3.org/Submission/wadl/wadl.xsd">
    <doc title="VPR Web Services">Hello World</doc>
    <resources base="${serverURL}/v1/">
        <doc title="Virtual Patient Record">Some stuff about this section</doc>

        <resource id="demographics" path="{pid}" type="#contentNegotiated">
            <doc title="Patient Demographics">
                <div xmlns="http://www.w3.org/1999/xhtml">
                    <div class="description">This resource contains patient demographics information for the specified
                        patient.
                    </div>
                    <table class="usageExamples">
                        <tr>
                            <td>${serverURL}/vpr/41205322</td>
                            <td>Fetch demographics for patient 41205322</td>
                        </tr>
                        <tr>
                            <td>${serverURL}/vpr/41205322?format=json</td>
                            <td>Fetch demographics for patient 41205322 formatted as JSON</td>
                        </tr>
                    </table>
                </div>
            </doc>
            <param name="pid" type="xsd:string" style="template" required="true">
                <doc title="a patient identifier">In Vista, the patient's id is their Internal Control Number or ICN
                    assigned by the Master Patient Index (MPI).
                </doc>
            </param>
            <method href="#getDemographics"/>

            <resource id="ptFeed" path="feed">
                <doc title="Patient Feed">
                    <div xmlns="http://www.w3.org/1999/xhtml">
                        <div class="description">This exposes the specified patients data as an atom feed.
                        </div>
                        <table class="usageExamples">
                            <tr>
                                <td>${serverURL}/vpr/00000222/feed</td>
                                <td>Fetch the feed for patient '00000222'</td>
                            </tr>
                        </table>
                    </div>
                </doc>
                <method href="#getPtFeed"/>
            </resource>

            <resource path="lab">
                <doc title="Lab Results"/>
                <resource id="labResult" path="show/{uid}" type="#contentNegotiated">
                    <doc title="Lab Result">
                        <div xmlns="http://www.w3.org/1999/xhtml">
                            <div class="description">This resource represents a specific lab result.</div>
                            <table class="usageExamples">
                                <tr>
                                    <td>${serverURL}/vpr/41205322/lab/show/CH;6919868.919994;2</td>
                                    <td>Fetch individual lab result with uid 'CH;6919868.919994;2'</td>
                                </tr>
                            </table>
                        </div>
                    </doc>
                    <param name="uid" type="xsd:string" style="template" required="true">
                        <doc title="a lab result identifier"/>
                    </param>
                    <method href="#getLabResult"/>
                </resource>
                <resource id="allLabs" path="all" type="#resultList #paginatedTemporalList #contentNegotiated">
                    <doc title="Lab Results">
                        <div xmlns="http://www.w3.org/1999/xhtml">
                            <div class="description">Use this URI to refer to all of a patients lab results. It will
                                return results in pages of 100 starting with the most recent and including embedded next
                                and
                                previous links (like search engine result pages).
                            </div>
                            <table class="usageExamples">
                                <tr>
                                    <td>${serverURL}/vpr/00000222/lab/all</td>
                                    <td>Fetch all lab results for patient '00000222'</td>
                                </tr>
                                <tr>
                                    <td>${serverURL}/vpr/00000222/lab/all?count=20&amp;amp;startIndex=40</td>
                                    <td>Fetch all lab results for patient '00000222' in pages of 20 starting at the 40th
                                        most recent result
                                    </td>
                                </tr>
                                <tr>
                                    <td>${serverURL}/vpr/00000222/lab/all?dateRange=200311-200312</td>
                                    <td>Fetch all lab results for patient '00000222' in November and December of
                                        2003
                                    </td>
                                </tr>
                                <tr>
                                    <td>${serverURL}/vpr/00000222/lab/all?dateRange=2005&amp;amp;count=100</td>
                                    <td>Fetch all lab results for patient '00000222' in 2005 in pages of 100.</td>
                                </tr>
                                <tr>
                                    <td>${serverURL}/vpr/00000222/lab/all?typeName=glucose</td>
                                    <td>Fetch all glucose results for patient '00000222'</td>
                                </tr>
                                <tr>
                                    <td>${serverURL}/vpr/00000222/lab/all?typeName=LDL&amp;amp;typeName=HDL</td>
                                    <td>Fetch LDL and HDL results for patient '00000222'</td>
                                </tr>
                            </table>
                        </div>
                    </doc>
                    <method href="#getLabResultList"/>
                </resource>
                <resource id="abnormalLabs" path="abnormal" type="#resultList #paginatedTemporalList #contentNegotiated">
                    <doc title="Abnormal Lab Results">
                        <div xmlns="http://www.w3.org/1999/xhtml">
                            <div class="description">This resource is a collection of the specified patients abnormal
                                lab results.
                            </div>
                            <table class="usageExamples">
                                <tr>
                                    <td>${serverURL}/vpr/00000222/lab/abnormal</td>
                                    <td>Fetch abnormal lab results for patient '00000222'</td>
                                </tr>
                                <tr>
                                    <td>${serverURL}/vpr/00000222/lab/abnormal?dateRange=200311-200312</td>
                                    <td>Fetch critical lab results for patient '00000222' in November and December of
                                        2003
                                    </td>
                                </tr>
                            </table>
                        </div>
                    </doc>
                    <method href="#getLabResultList"/>
                </resource>
                <resource id="criticalLabs" path="critical" type="#resultList #paginatedTemporalList #contentNegotiated">
                    <doc title="Critial Lab Results">
                        <div xmlns="http://www.w3.org/1999/xhtml">
