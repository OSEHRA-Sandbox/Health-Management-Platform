    <resource_type id="authenticated">
        <param name="Authentication" style="header"/>
    </resource_type>

    <resource_type id="contentNegotiated">
        <param name="format" style="query" required="false">
            <doc title="the preferred response format"/>
            <option value="xml" mediaType="application/xml"/>
            <option value="json" mediaType="application/json"/>
        </param>
    </resource_type>

    <resource_type id="paginatedTemporalList">
         <param name="dateRange" type="xsd:string" style="query" required="false">
            <doc title="A date range with which to limit results returned">
                    TBD: include a link to a page talking about HL7 date strings and range strings
            </doc>
        </param>
        <param name="count" type="xsd:integer" style="query" required="false">
            <doc title="The page size for a paged collection.">
                Defaults to 1000.
            </doc>
        </param>
        <param name="startIndex" type="xsd:integer" style="query" required="false">
            <doc title="An integer specifying the starting point when paging through a list of items.">
                Defaults to 0.
            </doc>
        </param>
    </resource_type>

    <resource_type id="resultList">
        <param name="typeName" type="xsd:string" style="query" required="false">
            <doc title="A result type name with which to limit the results returned."><!-- TODO: xsd:string is case sensitive, define our own schema include case insensitve string -->
                In VistA, this is the lab test name or name of the vital sign of interest.
            </doc>
        </param>
     </resource_type>
    
    <resource_type id="drugClass">
        <param name="drugClass" type="xsd:string" style="query" required="false">
            <doc title="A drug class name with which to limit the medications returned."><!-- TODO: xsd:string is case sensitive, define our own schema include case insensitve string -->
                In VistA, this is the drug class the medication is assigned to.
            </doc>
        </param>
    </resource_type>

    <method id="getDemographics" name="GET">
        <doc title="Returns patient demographics."/>
        <response status="200">
            <representation mediaType="application/xml">
                <doc title="Example XML response giving patient demographics"><![CDATA[
<patient>
    <link xmlns="http://www.w3.org/2005/Atom" rel="self"
          href="http://localhost:8080/cpe/vpr/41205322"/>
    <dateOfBirth>19670323</dateOfBirth>
    <familyName>Smith</familyName>
    <icn>41205322</icn>
    <gender>
        <link xmlns="http://www.w3.org/2005/Atom" rel="self"
              href="http://localhost:8080/cpe/codes/administrativeGender/M"/>
        <name>Male</name>
        <code>M</code>
    </gender>
    <givenNames>Joe</givenNames>
</patient>
                ]]></doc>
            </representation>
            <representation mediaType="application/json">
                <doc title="Example JSON response giving patient demographics"><![CDATA[

{
    link: {
        rel: "self",
        href: "http://localhost:8080/cpe/vpr/41205322"
    }
    dateOfBirth: "19670323",
    familyName: "Smith",
    icn: "41205322",
    givenNames: "Joe",
    gender: {
        name: "Male",
        code: "M,
        link: {
            rel: "self",
            href: "http://localhost:8080/cpe/codes/administrativeGender/M"
        }
    }
}
                ]]></doc>
            </representation>
        </response>
    </method>

    <method id="getPtFeed" name="GET">
        <doc title="Returns all activity related to a patient exposed as a feed."/>
        <response status="200">
            <representation mediaType="application/atom+xml"/>
        </response>
    </method>

    <method id="getLabResult" name="GET">
        <doc title="Returns information about an individual lab result."/>
        <response status="200">
            <representation mediaType="application/xml">

            </representation>
            <representation mediaType="application/json">

            </representation>
        </response>
    </method>

