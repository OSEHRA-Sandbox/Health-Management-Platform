        <doc title="Returns a list of lab results."/>
        <response status="200">
            <representation mediaType="application/xml">
                <doc title="Example XML response giving list of lab results">
                    <![CDATA[
<list total="37">
	<link xmlns="http://www.w3.org/2005/Atom" rel="self" href="http://localhost:8080/cpe/vpr/00000222/lab/all"/>
	<lab>
		<link xmlns="http://www.w3.org/2005/Atom" rel="self" href="http://localhost:8080/cpe/vpr/00000222/lab/14"/>
		<link xmlns="http://www.w3.org/2005/Atom" rel="http://vaww.cpe.DOMAIN.EXT/rels/patient" href="http://localhost:8080/cpe/vpr/00000222"/>
		<localAbbreviation>Glucose</localAbbreviation>
		<result>185</result>
		<localName>Glucose</localName>
		<status>
			<link xmlns="http://www.w3.org/2005/Atom" rel="self" href="http://localhost:8080/cpe/codes/resultStatus/completed"/>
			<name>Completed</name>
			<code>completed</code>
		</status>
		<collected>20100227</collected>
		<labId>14</labId>
		<facility>
			<name>CCHIT 2011 Ambulatory EHR Test Script</name>
			<code>CCHIT</code>
			<homeSite>true</homeSite>
		</facility>
	</lab>
	<lab>
		<link xmlns="http://www.w3.org/2005/Atom" rel="self" href="http://localhost:8080/cpe/vpr/00000222/lab/15"/>
		<link xmlns="http://www.w3.org/2005/Atom" rel="http://vaww.cpe.DOMAIN.EXT/rels/patient" href="http://localhost:8080/cpe/vpr/00000222"/>
		<localAbbreviation>LDH</localAbbreviation>
		<result>151</result>
